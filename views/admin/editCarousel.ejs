            <h1>Edit Carousel</h1>

            <form action="" method="post" enctype="multipart/form-data">
                <div class="mb-3">
                  <label for="name" class="form-label">Name</label>
                  <input type="name" class="form-control" id="name" name="carouselName" placeholder="Enter Name" value="<%= carousels[0].carouselName %>" required>
                </div>
              
                <div class="mb-3">
                  <label for="description" class="form-label">Carousel Description</label>
                  <input type="text" class="form-control" id="description" name="carouselDescription" placeholder="Enter Description" value="<%= carousels[0].carouselDescription %>" required>
                </div>
        
                        <div class="mb-3">
                            <label for="image" class="form-label">Photo</label>
                            <input type="file" name="carouselImages" value="<%= carousels[0].carouselImages %>" placeholder="Upload Files" multiple>
                            <br><br>
                        </div>
                    
                <input type="hidden" name="id" value="<%= carousels[0]._id %>">
                <button type="submit" class="btn btn-primary">Update User</button>
              </form>
